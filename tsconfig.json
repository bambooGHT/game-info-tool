{
  "compilerOptions": {
    "target": "es2020",
    "lib": [ "ESNext", ],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "outDir": "./dist",
    "baseUrl": "./",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
  }
}
